# OpenBB Mobile API - Environment Variables

# API Settings
APP_NAME=OpenBB Mobile API
APP_VERSION=2.0.0
API_PREFIX=/api/v2/mobile

# CORS Settings (configure appropriately for production)
CORS_ORIGINS=*

# Cache Settings
CACHE_ENABLED=true
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# Cache TTL (seconds)
CACHE_TTL_QUOTE=60
CACHE_TTL_SCREENER=900
CACHE_TTL_HISTORICAL=86400
CACHE_TTL_PROFILE=604800

# Pagination
DEFAULT_PAGE_SIZE=50
MAX_PAGE_SIZE=200

# Compression
GZIP_MIN_SIZE=1000

# OpenBB Settings
OPENBB_USER_DATA_PATH=
